    <entity name="Regression" class="org.ecoinformatics.seek.R.RExpression">
        <property name="expression" class="ptolemy.kernel.util.StringAttribute" value="eps &lt;- function(file, onefile=FALSE, horizontal=FALSE, paper=&quot;special&quot;, ...)&#10;  postscript(file=file, onefile=onefile, horizontal=horizontal, paper=paper, ...)&#10;&#10;&#10;if (is.character(Dependent)) {&#10;	Dependent &lt;- factor(Dependent)&#10;}&#10;&#10;model &lt;- lm(Dependent ~ Independent)&#10;&#10;R2 &lt;- summary(model)$r.squared&#10;&#10;plot(Independent, Dependent, xlab=&quot;GOES&quot;, ylab=&quot;CIMIS&quot;, asp=1)&#10;&#10;if (is.numeric(Dependent)) {&#10;	abline(model)&#10;	intercept &lt;- as.numeric(model$coefficient[1])&#10;	slope &lt;- as.numeric(model$coefficient[2])&#10;	title(main=paste(&quot;R^2 =&quot;, R2), sub=paste(&quot;b =&quot;, intercept, &quot;  m =&quot;, slope))&#10;	intercept&#10;	slope&#10;} &#10;if (is.character(Dependent)) {&#10;	ano &lt;- anova(model)&#10;	ano&#10;} &#10;&#10;eps(file=&quot;plot.eps&quot;, width=5, height=5)&#10;dev.off()&#10;&#10;">
